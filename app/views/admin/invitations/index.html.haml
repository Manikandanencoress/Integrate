%script{:type => "text/javascript"}
  -if !current_admin.studio.present?
    $("a.tn01").removeClass("tnOn");
    $("a.tn02").removeClass("tnOn");
    $("a.tn03").removeClass("tnOn");
    $("a.tn04").removeClass("tnOn");
    $("a.tn05").removeClass("tnOn");
    $("a.tn06").addClass("tnOn");
    $("#subnavlist2").hide();
    $("#subnavlist3").hide();
    $("#subnavlist4").hide();
    $("#subnavlist5").hide();
    $("#subnavlist6").show();
    $("a.sub04").addClass("subOn");
  -else
    $("a.tn02").addClass("tnOn");
    $("#subnavlist1").hide();
    $("#subnavlist2").show();
    $("#subnavlist3").hide();
    $("#subnavlist4").hide();
    $("#subnavlist5").hide();
    $("#subnavlist6").hide();
    $("a.sub03").addClass("subOn");
    
- content_for :breadcrumbs do
  = breadcrumbs(["Studios Home", [:admin, Studio]], "Admin Invitations").html_safe

.pageActions
  = link_to "Invite an admin", new_admin_invitation_path, :class => "butTextBlue"

%h1 Pending Milyoni Admin Invitations
= render :partial => "list"
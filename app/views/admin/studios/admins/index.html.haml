- content_for :breadcrumbs do
  = breadcrumbs(["Studios Home", [:admin, Studio]], ["#{ @studio.name } Home", [:admin, @studio]], "View Admins").html_safe

= render :partial => 'admin/studios/nav', :locals => {:active_tab => "Admins"}
.navTabContent

  .pageActions
    = link_to "Invite a #{@studio.name} Admin", new_admin_studio_invitation_path(@studio), :class => "butTextBlue"
    = link_to "Pending Invitations", admin_studio_invitations_path(@studio), :class => "butTextBlue"

  %h1 #{@studio.name} Admins
  %table.gridTable
    %tr
      %th Email
    - @admins.each do |admin|
      %tr
        %td #{admin.email}
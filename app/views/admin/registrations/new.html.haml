- title = @admin.invitation.studio ? @admin.invitation.studio.name: 'Milyoni'
%h2 #{title} Admin - Sign Up!

= semantic_form_for(@admin, :url => admin_registration_path) do |f|
  = devise_error_messages!
  = f.inputs do
    = f.input :email
    = f.input :password
    = f.input :password_confirmation
    = hidden_field_tag :token, @admin.invitation.token
    = hidden_field_tag :configuration_only, @admin.invitation.configuration_only
    = hidden_field_tag :reporting_only, @admin.invitation.reporting_only
  = f.submit "Create Account"